    padding-right: 12px;
}

QComboBox:editable:on {
    background: #f5f3ff;
}

QComboBox:!editable {
    background: #f9fafb;
}

QComboBox:focus {
    border: 1px solid #8b5cf6;
    outline: none;
    box-shadow: 0 0 0 2px rgba(139, 92, 246, 0.2);
}</string>
